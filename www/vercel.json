{
  "outputDirectory": "www",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/crm", "dest": "https://api.costruzionedigitale.com/crm" },
    { "src": "/crm/(.*)", "dest": "https://api.costruzionedigitale.com/crm/$1" },
    { "src": "/login", "dest": "https://api.costruzionedigitale.com/login" },
    { "src": "/api/check-auth", "dest": "https://api.costruzionedigitale.com/api/check-auth" },
    { "src": "/api/login", "dest": "https://api.costruzionedigitale.com/api/login" },
    { "src": "/api/logout", "dest": "https://api.costruzionedigitale.com/api/logout" },
    { "src": "/js/cookie-consent-manager.js", "dest": "/js/cookie-consent-manager.js" },
    { "src": "/:path", "dest": "/:path.html" },
    { "src": "/:path/", "dest": "/:path.html" },
    { "src": "/(.*)/", "dest": "/$1" },
    { "src": "/js/tracking.js", "dest": "/api/tracking.js" },
    { "src": "/api/:path*", "dest": "https://api.costruzionedigitale.com/api/:path*" }
  ]
}