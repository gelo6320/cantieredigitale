{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/crm", "destination": "https://api.costruzionedigitale.com/crm" },
    { "source": "/crm/(.*)", "destination": "https://api.costruzionedigitale.com/crm/$1" },
    { "source": "/login", "destination": "https://api.costruzionedigitale.com/login" },
    { "source": "/api/check-auth", "destination": "https://api.costruzionedigitale.com/api/check-auth" },
    { "source": "/api/login", "destination": "https://api.costruzionedigitale.com/api/login" },
    { "source": "/api/logout", "destination": "https://api.costruzionedigitale.com/api/logout" },
    { "source": "/js/cookie-consent-manager.js", "destination": "/js/cookie-consent-manager.js" },
    { "source": "/:path", "destination": "/:path.html" },
    { "source": "/:path/", "destination": "/:path.html" },
    { "source": "/(.*)/", "destination": "/$1" },
    { "source": "/js/tracking.js", "destination": "/api/tracking.js" },
    { "source": "/api/:path*", "destination": "https://api.costruzionedigitale.com/api/:path*" }
  ]
}